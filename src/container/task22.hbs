<div>
    <main>
        <div>
            {{#with page_header}}
            <h1 class="text-center text-capitalize">{{title}}</h1>
            <p class="text-center text-uppercase">{{description}}</p>
            {{/with}}

            {{#with section}}
            <div class="card">
                <div class="card-body">
                    {{#with about_us}}
                    <div>
                        {{#with title_list}}
                        <div class="text-end">
                            <span class="display-6">{{main}}</span>
                            <br>
                            <h2 class="text-end">{{sub}}</h2>
                        </div>
                        {{/with}}
                        <br>
                        <div>
                            {{#with button}}
                            <button class="btn btn-outline-info"> {{text}} </button>
                            {{/with}}
                        </div>
                    </div>
                    {{/with}}
                </div>
            </div>

            <br>

            <div class="card">
                <div class="card-body">
                    {{#with advantage}}
                    <div>
                        <div class="display-5 text-muted">{{title}}</div>
                        <br>
                        <div class="card">
                            <div class="card-body">
                                {{#with efficiency}}
                                <div class="display-4 text-muted">{{title}}</div>
                                <br>
                                <h6 class="text-uppercase">{{sub_title}}</h6>
                                <p class="text-capitalize">{{description}}</p>
                                <br>
                                <button class="btn btn-outline-warning"> {{button}} </button>
                                {{/with}}
                            </div>
                        </div>
                        <br>
                        <div class="card">
                            <div class="card-body">
                                {{#with innovation}}
                                <div class="text-center display-4 text-muted">{{title}}</div>
                                <br>
                                <h6 class="text-center text-uppercase">{{sub_title}}</h6>
                                <p class="text-center text-capitalize">{{description}}</p>
                                <br>
                                <button class="btn btn-outline-warning"> {{button}} </button>
                                {{/with}}
                            </div>
                        </div>

                        <br>

                        <div class="card">
                            <div class="card-body">
                                {{#with collaboration}}
                                <div class="text-end display-4 text-muted">{{title}}</div>
                                <br>
                                <h6 class="text-end text-uppercase">{{sub_title}}</h6>
                                <p class="text-end text-capitalize">{{description}}</p>
                                <br>
                                <button class="btn btn-outline-warning"> {{button}} </button>
                                {{/with}}
                            </div>
                        </div>
                    </div>
                    {{/with}}
                </div>
            </div>

            <br>

            <div class="card">
                <div class="card-body">
                    {{#with agreement}}
                    <div>
                        <div class="display-5">{{title}}</div>
                        <br>
                        <div class="card">
                            <div class="card-body">
                                {{#with info}}
                                <h5 class="text-dark">{{sub_title}}</h5>
                                <br>
                                <div class="card">
                                    <div class="card-body">
                                        {{#with detail}}
                                        <p class="text-dark">{{text}}</p>
                                        <br>
                                        <button class="btn btn-success"> {{button}} </button>
                                        {{/with}}
                                    </div>
                                </div>
                                {{/with}}
                            </div>
                        </div>
                    </div>
                    {{/with}}
                </div>
                {{/with}}
            </div>
        </div>
    </main>
</div>