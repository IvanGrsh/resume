<div>
    <main>
        <div class="card">
            <div class="card-body">
                {{#with heading}}
                <div class="card">
                    <div class="card-body">
                        {{#with main}}
                        <p class="display-4">{{title}}</p>
                        <h6 class="card-subtitle text-secondary"> text: {{text}}</h6>
                        <br>
                        <div>
                            {{#with button_text}}
                            <button class="btn btn-primary"> {{solid}} </button>
                            <br>
                            <br>
                            <button class="btn btn-outline-primary"> {{outline}} </button>
                            {{/with}}
                        </div>
                        {{/with}}
                    </div>
                </div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with second}}
                        <p class="display-4">{{title}}</p>
                        <h6 class="card-subtitle text-secondary"> text: {{text}}</h6>
                        <br>
                        <div>
                            {{#with button_text}}
                            <button class="btn btn-secondary"> {{solid}} </button>
                            <br>
                            <br>
                            <button class="btn btn-outline-secondary"> {{outline}} </button>
                            {{/with}}
                        </div>
                        {{/with}}
                    </div>
                </div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with subblock}}
                        <p class="display-4">{{title}}</p>
                        <h6 class="card-subtitle text-secondary"> text: {{text}}</h6>
                        <br>
                        <div>
                            {{#with button_text}}
                            <button class="btn btn-dark"> {{solid}} </button>
                            <br>
                            <br>
                            <button class="btn btn-outline-dark"> {{outline}} </button>
                            {{/with}}
                        </div>
                        {{/with}}
                    </div>
                </div>
                {{/with}}
            </div>
        </div>

        <br>

        <div class="card">
            <div class="card-body">
                {{#with nested_display}}
                <div>
                    <p class="display-4">{{title}}</p>
                </div>
                <div class="card">
                    <div class="card-body">
                        {{#with children_display}}
                        <p class="display-6">{{title}}</p>
                        <h6 class="card-subtitle text-secondary"> text: {{text}}</h6>
                        <br>
                        <div>
                            {{#with button}}
                            <button class="btn btn-outline-info"> {{text}} </button>
                            {{/with}}
                        </div>
                        {{/with}}
                        <br>
                        <h6 class="card-subtitle"> {{header_text}}</h6>
                        <br>
                        {{#with button}}
                        <button class="btn btn-outline-success"> {{text}} </button>
                        {{/with}}
                    </div>
                </div>
                {{/with}}
            </div>
        </div>
        <br>
        <div class="card">
            <div class="card-body">
                {{#with paragraph}}
                <div>
                    <p class="text-uppercase">Paragraph</p>
                    <p class="text-lowercase">{{title}}</p>
                    <p class="text-lowercase">"{{text}}"</p>
                </div>
                <div>
                    {{#with button}}
                    <a href="{{url}}" class="btn btn-link">
                        {{text}}
                    </a>
                    {{/with}}
                </div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#each list_name}}
                        <ul>
                            <li>
                                <a href="{{url}}" class="btn btn-link">
                                    {{text}}
                                </a>
                            </li>
                        </ul>
                        {{/each}}
                    </div>
                </div>
                {{/with}}
            </div>
        </div>
    </main>
</div>